{
    "manifest_version": 3,
    "name": "Local Tools",
    "version": "1.0",
    "description": "Extracts Keyword, KD, and SV data from Ahrefs reports",
    "permissions": [
      "activeTab",
      "scripting",
      "clipboardWrite"
    ],
    "action": {
      "default_title": "Local Tools",
      "default_popup": "popup.html"
    }
  }