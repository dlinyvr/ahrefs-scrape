{
    "manifest_version": 3,
    "name": "aaBlog & TikTok Content Extractor",
    "version": "1.0",
    "description": "Extracts blog content and TikTok URLs",
    "permissions": [
      "activeTab",
      "scripting",
      "clipboardWrite"
    ],
    "action": {
      "default_title": "Content Extractor",
      "default_popup": "popup.html"
    }
  }